/*========== User Custom CSS Styles ==========*/
nav#main-navigation .menu > li {
padding: 0 10px 0;
}
.header-right ul.social-icons{
margin:0;
float:right;
width:100%;
}
.header-right ul.social-icons li {
float:right;
}
#header-section.header-6 #header{
padding: 5px 0 0;}

.infographic{
width:100%;
display:flex;
flex-direction:collumn;
justify-content:center;
align-items-center;
}

.infographic .infographic-step{
flex:1;
text-align:center;
height:350px;
}
.infographic .infographic-step .sf-icon-cont{
margin:0;
}
.infographic .infographic-step .step-segment{
width:100%;
height:25px;
background-color:#1dc6df;
position:relative;
}
.infographic .infographic-step:nth-child(2n) .step-segment{
background-color:#f1803c;
}

.infographic-step{
display:flex;
flex-direction:column;
}
.infographic-step .infographic-content{
flex:1;
display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
}

.infographic-step .infographic-content h3, .infographic-step .infographic-content p{
margin:0;
}
.infographic-step .infographic-content h3 {
text-transform:uppercase;
font-size:24px;
margin: 10px 0;

}
.infographic .infographic-step:nth-child(2n+1) .step-segment:after{
content:"";
width:6px;
height:55px;
position:absolute;
background-color:#1dc6df;
left:auto;
top:0;
border-radius: 0 0 3px 3px;
}
.infographic .infographic-step:nth-child(2n) .step-segment:after{
content:"";
width:6px;
height:55px;
position:absolute;
background-color:#f1803c;
left:auto;
bottom:0;
border-radius: 3px 3px 0 0;
}
.infographic-step:first-child .step-segment{
border-radius: 15px 0 0 15px;
}
.infographic-step:last-child .step-segment{
border-radius: 0 15px 15px 0;
}
.sf-icon-accent .sf-icon{
color:#f1803c;
}
